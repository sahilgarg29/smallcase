    <style>
      #body {
        padding: 0;
        margin: 0;
        font-size: 16px;
        font-family: sans-serif;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        text-rendering: auto;
        color: #535b62;
        overflow-x: hidden;
        width: 80%;
        margin: auto;
      }
      #main_container {
        /* width: 98%; */
        /* border: 1px solid red; */
        display: grid;
        grid-template-columns: 70% 20%;
        gap: 60px;
      }
 
      /* #leftTop {
        border: 1px solid red;
      } */
      #rightTop {
        border: 1px solid #9c9ea0;
        line-height: 25px;
        padding: 12px;
        border-radius: 5px;
        margin-top: 25%;
        font-size: 14px;
      }
      #leftBottom {
        /* border: 1px solid red; */
        margin-top: 40px;
      }
      #rightBottom {
        border: 1px solid #9c9ea0;
        line-height:    25px;
        margin-top: 30px;
        padding: 12px;
        border-radius: 5px;
        font-size: 14px;
      }
      @media only screen and (min-width:321px) and (max-width:720px){
        #main_container {
          grid-template-columns: repeat(1,1fr);
          grid-template-rows: repeat(4,300px);
        }
        #leftBottom {
        /* border: 1px solid red; */
        margin-top: 50%;
        }
        #leftBottom{
          display: none;
        }
        #rightTop{
          display: block;
        }
      }
      .blue {
        color: blue;
      }
      #bluebar {
        width: 100%;
        color: rgb(212, 93, 93);
        border-radius: 2px;
        height: 20px;
      }
      #leftTop div {
        /* border: 1px solid red; */
        height: 75px;
        margin-bottom: 20px;
      }
      #manimg {
        width: 500px;
      }
      #leftBottom div {
        display: inline-block;
      }
      #mantext {
        vertical-align: top;
      }
      #insights {
        display: grid;
        grid-template-columns: 45% 35% 15%;
        /* border: 1px solid red; */
        gap: 15px;
      }
      #logotext {
        display: grid;
        grid-template-columns: 20% 75%;
        /* border: 1px solid green; */
      }
      #img1_1 {
        width: 80%;
        border-radius: 5px;
      }
      #lower {
        font-size: 14px;
        color: gray;
        font-weight: 100;
      }
      #lower1 {
        margin-top: 3px;
        color: rgb(61, 59, 59);
        font-size: 17px;
        font-weight: 550;
        line-height: 22px;
      }
      #lower1 a {
        text-decoration: none;
        color: rgb(61, 59, 59);
        font-size: 17px;
      }
      #row1 {
        padding: 0 4px;
      }
      #btn {
        background-color: white;
        color: #1f7ae0;
        padding: 10px 12px;
        border-radius: 4px;
        border-color: #1f7ae0;
      }
      #btn1 button {
        justify-content: center;
        justify-items: center;
        margin-top: 15%;
      }
      #rthead {
        font-size: 16px;
      }
      #itis {
        font-size: 24px;
        font-weight: 500;
      }
      #watch {
        color: #81878c;
        font-weight: 500;
        line-height: 28px;
      }
      /* //hover */
      .dropbtn {
        color: 81878c;
        /* padding: 16px; */
        font-size: 14px;
        border: none;
      }

      .dropdown {
        position: relative;
        display: inline-block;
      }

      .dropdown-content {
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        z-index: 1;
        font-size: 14px;
        line-height: 18px;
      }

      .dropdown-content a {
        color: black;
        padding: 10px;
        text-decoration: none;
        display: block;
      }

      .dropdown-content a:hover {
        background-color: #ddd;
      }

      .dropdown:hover .dropdown-content {
        display: contents;
      }
      /* hoverr */
      /* //mainnavbar */
      #navbar {
        display: flex;
        height: 50px;
        border-bottom: 1px solid #999da2;
        font-size: 14px;
      }
      #logo {
        margin-top: 10px;
        height: 30px;
        /* margin-left: 10%; */
      }
      .navtext {
        color: #999da2;
        text-decoration: none;
        margin-left: 3%;
        margin-top: 15px;
      }
      #separation {
        margin-left: 23%;
      }
      .navbut {
        text-decoration: none;
        margin-top: 15px;
        margin-left: 3%;
        background-color: white;
        border: none;
        color: #81878c;
        font-size: 18px;
      }
      #navq {
